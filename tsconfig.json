{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
	"baseUrl": ".",
    "incremental": true,
	"paths": {
		"@/*": ["*"],
	}
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/api/workspace/[id]/settings/users/add.t.tss"],
  "exclude": ["node_modules"]
}
